{
  "name": "reclipboard",
  "version": "1.0.0",
  "description": "Regex Clipboard",
  "main": "main.js",
  "scripts": {
    "test": "electron .",
    "package-win": "electron-packager . reclipboard --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icon_black.ico --prune=true --out=release-builds --version-string.CompanyName=\"Fiend3d\" --version-string.FileDescription=\"RegEx Clipboard\" --version-string.ProductName=\"REClipboard\"",    
    "package-linux": "electron-packager . reclipboard --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon_black.png --prune=true --out=release-builds" 
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fiend3d/REClipboard.git"
  },
  "keywords": [
    "regex",
    "clipboard",
    "crossplatform"
  ],
  "author": "Vlad Tagintsev",
  "license": "MIT",
  "dependencies": {
    "electron-packager": "^9.1.0",
    "jsonfile": "^4.0.0"
  },
  "devDependencies": {
    "electron": "1.7.9"
  }
}
